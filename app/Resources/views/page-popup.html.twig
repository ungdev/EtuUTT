{% extends '::layout-popup.html.twig' %}

{% block page %}
    <div class="container">
        {% if message %}
            <div class="page-alert-mobile alert alert-{{ message.type }}">
                {{ message.message|trans }}
            </div>
        {% endif %}

        <div class="content content-full padding20-all">
            {% block content %}{% endblock %}
        </div>
    </div>
{% endblock %}
